	"apis": [],
	"name": "HRIA_FETCH_JOB_DETAILS",
	"ver": "22.7.7.1624",
	"description": "script to fetch job title, function and years of Experience.",
	"content": [
		"==OBJECT STRING==",
		"async function fetch_job_details() {\r",
		"\r",
		"    try {\r",
		"\r",
		"\r",
		"        let startTime = Date.now();\r",
		"\r",
		"        let jobID = req.body && req.body.jobID;\r",
		"\r",
		"        if (jobID === undefined) {\r",
		"            fail();\r",
		"        }\r",
		"\r",
		"        const queryWhere = 'job.jobID = :jobID';\r",
		"        const queryValues = {\r",
		"            jobID: jobID\r",
		"        };\r",
		"\r",
		"\r",
		"        //actual query\r",
		"        const job_details = await entities.hria_jobs.createQueryBuilder(\"job\")\r",
		"            //no select statement\r",
		"            .where(queryWhere, queryValues)\r",
		"            .getOne();\r",
		"\r",
		"\r",
		"        result = {\r",
		"            jobDetails: job_details,\r",
		"            endTime: 'operation executed in ' + ((Date.now() - startTime) / 1000) + ' seconds'\r",
		"        }\r",
		"\r",
		"        console.log(result);\r",
		"\r",
		"        log.info('[HRIA_FETCH_JOB_DETAILS] script executed with jobs -- ' + job_details);\r",
		"        complete();\r",
		"\r",
		"\r",
		"    } catch (err) {\r",
		"\r",
		"        log.error('[HRIA_FETCH_JOB_DETAILS] script execution failed due to -- ' + err);\r",
		"        fail();\r",
		"\r",
		"    }\r",
		"\r",
		"}\r",
		"\r",
		"await fetch_job_details();"
	],
