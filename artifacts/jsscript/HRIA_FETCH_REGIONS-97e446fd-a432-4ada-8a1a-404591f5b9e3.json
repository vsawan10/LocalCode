	"name": "HRIA_FETCH_REGIONS",
	"ver": "22.7.6.1859",
	"description": "fetch all available regions available",
	"content": [
		"==OBJECT STRING==",
		"async function fetchRegions() {\r",
		"    try {\r",
		"        //getting all the regions \r",
		"        const regions = await entities.hria_regions.find({\r",
		"            select: [\r",
		"                'regionName'\r",
		"            ],\r",
		"            order: {\r",
		"                regionName: \"ASC\",\r",
		"            }\r",
		"        });\r",
		"\r",
		"        let regionArr = [];\r",
		"\r",
		"        for (let i = 0; i < regions.length; i++) {\r",
		"            regionArr[i] = regions[i].regionName;\r",
		"        }\r",
		"\r",
		"        result = {\r",
		"            regionArr: regionArr\r",
		"        }\r",
		"\r",
		"        log.info('[HRIA_FETCH_REGIONS] script executed -- fetched the following regions: ' + regions);\r",
		"        complete();\r",
		"\r",
		"    } catch (err) {\r",
		"\r",
		"        log.error('execution of [HRIA_FETCH_REGIONS] script failed due to -- ' + err);\r",
		"        fail();\r",
		"    }\r",
		"}\r",
		"\r",
		"await fetchRegions();\r",
		""
	],
