	],
	"apis": [],
	"name": "HRIA_FETCH_ALL_COUNTRIES",
	"ver": "22.7.25.117",
	"description": "script to fetch all the possible countries maintained in config",
	"content": [
		"==OBJECT STRING==",
		"async function fetch_all_countries() {\r",
		"\r",
		"    try {\r",
		"        let startTime = Date.now();\r",
		"\r",
		"        //find operation to fetch all the possible configurable list of countries maintained.\r",
		"        let countryData = [];\r",
		"        countryData = await entities.hria_country.find({\r",
		"            select: [\r",
		"                'country'\r",
		"            ],\r",
		"            order: {\r",
		"                country: \"ASC\"\r",
		"            }\r",
		"        });\r",
		"\r",
		"        let formattedResponse = [];\r",
		"        if (countryData.length > 0) {\r",
		"            formattedResponse = await formatData(countryData);\r",
		"        }\r",
		"\r",
		"\r",
		"        result = {\r",
		"            dataObj: formattedResponse,\r",
		"            type: 'ALL_COUNTRIES',\r",
		"            queryMsg: 'operation completed in ' + ((Date.now() - startTime) / 1000) + ' seconds'\r",
		"        }\r",
		"\r",
		"        //console.log(result);\r",
		"\r",
		"\r",
		"        log.info('[HRIA_FETCH_ALL_COUNTRIES] script executed by fetching following a total of ' + formattedResponse.length + ' countries -- ' + formattedResponse);\r",
		"        complete();\r",
		"\r",
		"    } catch (err) {\r",
		"\r",
		"        log.error('[HRIA_FETCH_ALL_COUNTRIES] script execution failed due to -- ' + err);\r",
		"        fail();\r",
		"\r",
		"    }\r",
		"\r",
		"}\r",
		"\r",
		"async function formatData(dataList) {\r",
		"\r",
		"    let arr = [];\r",
		"\r",
		"    for (let i = 0; i < dataList.length; i++) {\r",
		"        arr[i] = dataList[i].country.trim();\r",
		"    }\r",
		"\r",
		"    return arr;\r",
		"\r",
		"}\r",
		"\r",
		"await fetch_all_countries();"
	],
