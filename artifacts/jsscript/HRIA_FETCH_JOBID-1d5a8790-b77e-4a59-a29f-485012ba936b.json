	"apis": [],
	"name": "HRIA_FETCH_JOBID",
	"ver": "22.7.6.1910",
	"description": "script to fetch the jobIDs",
	"content": [
		"==OBJECT STRING==",
		"async function fetchJobIDs() {\r",
		"\r",
		"    try {\r",
		"\r",
		"        let startTime = Date.now();\r",
		"\r",
		"        //query to fetch all the jobIDs\r",
		"        const jobs = await entities.hria_jobs.find({\r",
		"            select: [\r",
		"                'jobID'\r",
		"            ],\r",
		"            order: {\r",
		"                jobID: \"ASC\"\r",
		"            }\r",
		"        });\r",
		"\r",
		"\r",
		"        let jobArr = [];\r",
		"        for (let j = 0; j < jobs.length; j++) {\r",
		"            jobArr[j] = jobs[j].jobID;\r",
		"        }\r",
		"\r",
		"        result = {\r",
		"            jobArr: jobArr,\r",
		"            endTime: 'executed in ' + ((Date.now() - startTime) / 1000) + ' seconds'\r",
		"        }\r",
		"\r",
		"        console.log(result);\r",
		"\r",
		"        log.info('[HRIA_FETCH_JOBID] script executed & fetch jobs -- ' + jobArr)\r",
		"        complete();\r",
		"\r",
		"    } catch (err) {\r",
		"\r",
		"        log.error('[HRIA_FETCH_JOBID] script execution failed due to -- ' + err);\r",
		"        fail();\r",
		"\r",
		"\r",
		"    }\r",
		"}\r",
		"\r",
		"await fetchJobIDs();"
	],
